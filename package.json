{
  "name": "vue-skeleton-mvp",
  "version": "4.1.3",
  "description": "VueJS Vuetify Vue Router Vuex - Basic Project Skeleton - Starter Web App. Written on JavaScript using async/await.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davellanedam/vue-skeleton-mvp.git"
  },
  "scripts": {
    "generate-build-version": "node generate-build-version",
    "serve": "vue-cli-service serve",
    "build": "npm run generate-build-version && vue-cli-service build",
    "format": "prettier --write --config .prettierrc \"**/*.js\"",
    "lint": "vue-cli-service lint --fix",
    "test:e2e": "vue-cli-service test:e2e --mode e2e",
    "test:unit": "vue-cli-service test:unit",
    "remark": "remark . -o",
    "ci": "vue-cli-service test:e2e --env API_URL=www.edtaee.com,ENV=ci --headless --config video=false",
    "start": "node main.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run remark"
    }
  },
  "dependencies": {
    "@mdi/font": "^3.9.97",
    "@vuelidate/validators": "^2.0.0-alpha.24",
    "axios": "^0.19.2",
    "date-fns": "^1.30.1",
    "grapesjs": "^0.17.27",
    "jquery": "^3.6.0",
    "nodemailer": "^6.4.8",
    "remark-cli": "^7.0.1",
    "vee-validate": "^2.2.15",
    "vue": "^2.6.11",
    "vue-advanced-cropper": "^1.3.0",
    "vue-analytics": "^5.22.1",
    "vue-country-flag": "^1.3.1",
    "vue-i18n": "^8.18.2",
    "vue-meta": "^2.3.4",
    "vue-recaptcha": "^1.3.0",
    "vue-resize-text": "^0.1.1",
    "vue-router": "^3.3.2",
    "vue-tel-input-vuetify": "^1.2.16",
    "vue-upload-component": "^2.8.22",
    "vuelidate": "^0.7.6",
    "vuetify": "^1.5.24",
    "vuetify-confirm": "^0.2.6",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.4.1",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-e2e-cypress": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-unit-mocha": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-prettier": "^5.1.0",
    "@vue/test-utils": "^1.0.3",
    "awesome-phonenumber": "^2.47.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-formidable": "^4.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "7.20.0",
    "faker": "^4.1.0",
    "husky": "^3.1.0",
    "prettier": "^1.19.1",
    "sass": "^1.26.8",
    "sass-loader": "^7.3.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-i18n": "^0.6.1",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.4",
    "whatwg-fetch": "^3.6.2"
  },
  "keywords": [
    "vuejs",
    "vue-router",
    "vuex",
    "vuetify",
    "javascript",
    "i18n",
    "eslint",
    "skeleton",
    "async",
